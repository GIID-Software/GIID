{
  "name": "giid",
  "version": "0.0.1",
  "description": "A powerfull discord bot.",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node dist/index.js",
    "start:dev": "concurrently --names \"TSC,NODEMON\" \"npm run build:dev\" \"npm run nodemon\" --kill-others",
    "postinstall": "npm run build",
    "build:html": "nodemon -e ejs,css --exec \"copyfiles -u 1 src/**/*.ejs src/**/*.css dist/\" --ignore dist/",
    "nodemon": "nodemon dist/index.js",
    "build": "npm run clean && npm run build:prod",
    "build:prod": "npm run clean && npx tsc",
    "build:dev": "npm run clean && npx tsc --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GIID-Software/GIID"
  },
  "keywords": [
    "discord"
  ],
  "author": "mattsivak",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@types/ejs": "^3.1.0",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.4.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.14",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "tailwindcss": "^3.0.2",
    "typescript": "^4.5.2"
  }
}
